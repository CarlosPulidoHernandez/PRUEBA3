<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= user.surname %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Creationdate:</strong>
    <%= user.creationDate %>
  </p>

  <p>
    <strong>Isenable:</strong>
    <%= user.isEnable %>
  </p>

</div>
